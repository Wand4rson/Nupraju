    SQL.Strings = (
      'SELECT'
      '  oc.ocr_codigo,'
      '  oc.alu_matricula,'
      '  al.alu_nome,'
      '  oc.ocr_datalancamento,'
      '  oc.ocr_horalancamento,'
      '  oci.ocr_item_nroprocesso,'
      '  oci.cliente_codigo'
      'FROM '
      'ocorrencia oc'
      'INNER JOIN alunos al ON(al.alu_matricula = oc.alu_matricula)'
      
        'INNER JOIN ocorrencia_itens oci ON(oc.ocr_codigo = oci.ocr_codig' +
        'o)'
      'GROUP BY oc.ocr_codigo ORDER BY oc.ocr_codigo DESC ')
